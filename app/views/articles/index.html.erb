<div class="container" style="margin-top: 70px">
  <div class="row">
<div class="page-header">
  <h1>Articles</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Publish time</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= link_to article.title, article %></td>
        <td><%= article.url %></td>
        <td><%= article.publish_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  </div>
</div>
